{
  "name": "dexterdb",
  "version": "2.0.2",
  "description": "Dexter DB- Generate realistic test data for MongoDB databases using Mongoose",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "dexterdb": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node cli.ts",
    "dev:watch": "nodemon --watch src --watch cli.ts --exec ts-node cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src cli.ts --ext .ts",
    "lint:fix": "eslint src cli.ts --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"cli.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"cli.ts\" \"tests/**/*.ts\"",
    "test:seeder": "ts-node test-seeder.ts",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:logs": "docker-compose logs -f mongodb",
    "db:reset": "docker-compose down -v && docker-compose up -d"
  },
  "keywords": [
    "database",
    "dextedbr",
    "seeder",
    "faker",
    "test-data",
    "mongoose",
    "mongodb",
    "odm",
    "data-generation"
  ],
  "author": "Daniel Idowu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/daniel-idowu-01/dexterdb.git"
  },
  "bugs": {
    "url": "https://github.com/daniel-idowu-01/dexterdb/issues"
  },
  "homepage": "https://github.com/daniel-idowu-01/dexterdb#readme",
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "mongoose": "^8.0.0",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "ora": "^5.4.1",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.2",
    "prisma": "^5.19.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
